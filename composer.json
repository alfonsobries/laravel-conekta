{
    "name": "alfonsobries/laravel-conekta-cashier",
    "description": "Laravel Conekta provides an expressive, fluent interface to Conekta's subscription billing services.",
    "keywords": ["laravel", "conekta", "billing"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/alfonsobries/laravel-conekta-cashier/issues",
        "source": "https://github.com/alfonsobries/laravel-conekta-cashier"
    },
    "authors": [
        {
            "name": "Alfonso Bribiesca",
            "email": "alfonso@vexilo.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "conekta/conekta-php": "dev-php7.2-support",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/database": "~5.3",
        "illuminate/support": "~5.3",
        "nesbot/carbon": "~1.0",
        "symfony/http-kernel": "~2.7|~3.0|~4.0"
    },
    "require-dev": {
        "illuminate/http": "~5.3",
        "illuminate/routing": "~5.3",
        "illuminate/view": "~5.3",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.0",
        "vlucas/phpdotenv": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Alfonsobries\\ConektaCashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alfonsobries\\ConektaCashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.0-dev"
        },
        "laravel": {
            "providers": [
                "Alfonsobries\\ConektaCashier\\LaravelConektaCashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/conekta/conekta-php.git"
        }
    ]
}
